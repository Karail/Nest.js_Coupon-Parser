{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/coupons/src/v1",
  "monorepo": true,
  "root": "apps/coupons",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/coupons/tsconfig.app.json"
  },
  "projects": {
    "coupons": {
      "type": "application",
      "root": "apps/coupons",
