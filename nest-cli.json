      "sourceRoot": "apps/coupons/src",
      "compilerOptions": {
        "tsConfigPath": "apps/coupons/tsconfig.app.json"
      }
    },
    "decode-deep-link": {
      "type": "application",
      "root": "apps/decode-deep-link",
      "entryFile": "main",
      "sourceRoot": "apps/decode-deep-link/src",
      "compilerOptions": {
        "tsConfigPath": "apps/decode-deep-link/tsconfig.app.json"
      }
    }
  }
}
