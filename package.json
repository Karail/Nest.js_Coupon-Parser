{
  "name": "coupon-parser",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && nest build decode-deep-link",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:coupons": "nest start",
    "start:dev:coupons": "nest start --watch",
    "start:debug:coupons": "nest start --debug --watch",
    "start:prod:coupons": "node dist/apps/coupons/v1/main",
    "start:decode-deep-link": "nest start decode-deep-link",
    "start:dev:decode-deep-link": "nest start decode-deep-link --watch",
    "start:debug:decode-deep-link": "nest start decode-deep-link --debug --watch",
    "start:prod:decode-deep-link": "node dist/apps/decode-deep-link/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/coupon-parser/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/bull": "^0.1.2",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/microservices": "^7.4.4",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.1",
    "@sindresorhus/slugify": "^1.1.0",
    "bull": "^3.18.0",
    "cloudinary": "^1.23.0",
    "deeplink-module": "^1.0.12",
    "franc": "^5.0.0",
    "mongoose": "^5.10.5",
    "nest-emitter": "^1.1.1",
    "node-fetch": "^2.6.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.4.4",
    "@types/bull": "^3.14.2",
    "@types/express": "^4.17.3",
    "@types/franc": "^4.0.1",
    "@types/jest": "25.1.4",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^13.9.1",
    "@types/node-fetch": "^2.5.7",
    "@types/supertest": "^2.0.8",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
